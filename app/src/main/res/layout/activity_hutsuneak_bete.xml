        app:layout_constraintEnd_toStartOf="@+id/guiavertica1"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="@+id/guiavertica2"
        app:layout_constraintTop_toTopOf="@+id/guiahorizonta6"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewHutsuneak1"
        android:layout_width="105dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="56dp"
        android:text="@string/p5_hutsuneak_bete_1"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/guiahorizonta4"
        app:layout_constraintEnd_toStartOf="@+id/guiavertica3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guiavertica2"
        app:layout_constraintTop_toTopOf="@+id/guiahorizonta2"
