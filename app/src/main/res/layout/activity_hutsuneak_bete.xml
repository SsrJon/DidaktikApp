        android:layout_height="100dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/guiahorizontal"
        app:layout_constraintEnd_toStartOf="@+id/guiavertica1"
        app:layout_constraintStart_toStartOf="@+id/guiavertica2"
        app:layout_constraintTop_toTopOf="@+id/guiahorizonta3">

        <Button
            android:id="@+id/btn1"
            android:layout_width="354dp"
            android:layout_height="25dp"
            android:layout_weight="1"
            android:background="#DACFCF"
            android:text="@string/p5_hutsuneak_bete_r1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.786" />

        <Button
