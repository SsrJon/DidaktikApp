        android:layout_height="40dp"
        android:text="@string/p5_hutsuneak_bete_2"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/guiahorizonta4"
        app:layout_constraintEnd_toStartOf="@+id/guiavertica1"
        app:layout_constraintHorizontal_bias="0.617"
        app:layout_constraintStart_toEndOf="@+id/target1"
        app:layout_constraintTop_toTopOf="@+id/guiahorizonta2"
